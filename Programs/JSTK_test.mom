LOOP_START
	BRJU COUNT_UP
	NOP
	BRJD COUNT_DOWN
	NOP
	JMP LOOP_START
	MOV R2 0
COUNT_UP
	LDAV R0 0
	MOV R1 1
	CMP R2 R1
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 0
	JMP LOOP_START
	MOV R2 1
COUNT_DOWN
	LDAV R0 0
	MOV R1 -1
	CMP R1 R2
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 0
	JMP LOOP_START
	MOV R2 -1


LOOP_START
	BRJU COUNT_UP
	NOP
	BRJD COUNT_DOWN
	NOP
	BRJL COUNT_LEFT
	NOP
	BRJR COUNT_RIGHT
	NOP
	BRB1 COUNT_B1
	NOP
	BRB2 COUTN_B2
	NOP
	JMP LOOP_START
	MOV R2 0
COUNT_UP
	LDAV R0 0
	MOV R1 1
	CMP R2 R1
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 0
	JMP LOOP_START
	MOV R2 1
COUNT_DOWN
	LDAV R0 0
	MOV R1 -1
	CMP R1 R2
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 0
	JMP LOOP_START
	MOV R2 -1
COUNT_LEFT
	LDAV R0 1
	MOV R1 1
	CMP R1 R2
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 1
	JMP LOOP_START
	MOV R2 1
COUNT_RIGHT
	LDAV R0 1
	MOV R1 -1
	CMP R1 R2
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 1
	JMP LOOP_START
	MOV R2 -1
COUNT_B1
	LDAV R0 2
	MOV R1 1
	CMP R1 R2
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 2
	JMP LOOP_START
	MOV R2 1
COUTN_B2
	LDAV R0 2
	MOV R1 -1
	CMP R1 R2
	BRZ LOOP_START
	NOP
	ADD R0 R0 R1
	STRV R0 2
	JMP LOOP_START
	MOV R2 -1
